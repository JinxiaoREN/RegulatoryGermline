bash generate_command.gatk.sh reference/TCGA_WGS_gspath_WWL_Mar2018.HighPass.normal.txt HighPass 1500
bash generate_command.gatk.sh reference/TCGA_WGS_gspath_WWL_Mar2018.LowPass.normal.txt LowPass 100
bash generate_command.varscan.sh reference/TCGA_WGS_gspath_WWL_Mar2018.HighPass.normal.txt HighPass 1500
bash generate_command.varscan.sh reference/TCGA_WGS_gspath_WWL_Mar2018.LowPass.normal.txt LowPass 100
for i in `ls cloud/gcloud_command.*`; do cat $i | awk 'NR>1{print $0}' | head -n 10 >> cloud/firstbatch.sh; done
bash cloud/firstbatch.sh 2> cloud/log
